{
    "name": "urban-transportation-json-new",
    "description": "Please enter a brief description here",
    "version": "0.0.1-SNAPSHOT",
    "files": [
        "bin",
        "out",
        "src"
    ],
    "type": "module",
    "scripts": {
        "build": "tsc -b tsconfig.src.json && node esbuild.mjs",
        "watch": "concurrently -n tsc,esbuild -c blue,yellow \"tsc -b tsconfig.src.json --watch\" \"node esbuild.mjs --watch\"",
        "lint": "eslint src --ext ts",
        "langium:generate": "langium generate",
        "langium:generate:production": "langium generate --mode=production",
        "langium:watch": "langium generate --watch",
        "vscode:prepublish": "npm run build && npm run lint",
        "start:experiment": "npm run build && node ./out/experimentation/main.js",
        "build:web": "npm run build",
        "bundle": "vite build",
        "bundle:serve": "http-server ./dist --port 5175",
        "dev": "vite",
        "dev:debug": "vite --debug --force",
        "serve": "npm run dev",
        "test": "vitest run"
    },
    "dependencies": {
        "@codingame/monaco-vscode-editor-service-override": "~3.2.3",
        "@codingame/monaco-vscode-keybindings-service-override": "~3.2.3",
        "@langchain/core": "^0.3.30",
        "@langchain/google-genai": "^0.1.6",
        "@langchain/ollama": "^0.2.0",
        "ajv": "^8.17.1",
        "chalk": "~5.3.0",
        "commander": "~11.0.0",
        "dotenv": "^16.4.5",
        "langchain": "^0.3.11",
        "langium": "~3.3.0",
        "monaco-editor": "npm:@codingame/monaco-vscode-editor-api@~3.2.3",
        "monaco-editor-wrapper": "~4.0.2",
        "monaco-languageclient": "~8.1.1",
        "vscode": "npm:@codingame/monaco-vscode-api@~3.2.3",
        "vscode-languageclient": "~9.0.1",
        "vscode-languageserver": "~9.0.1",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@codingame/esbuild-import-meta-url-plugin": "~1.0.2",
        "@types/lodash": "^4.17.15",
        "@types/node": "^18.0.0",
        "@types/vscode": "~1.67.0",
        "@typescript-eslint/eslint-plugin": "~7.3.1",
        "@typescript-eslint/parser": "~7.3.1",
        "concurrently": "~8.2.1",
        "esbuild": "~0.20.2",
        "eslint": "~8.57.0",
        "http-server": "~14.1.1",
        "langium-cli": "~3.3.0",
        "typescript": "~5.1.6",
        "vite": "~5.2.7",
        "vitest": "~1.4.0"
    },
    "volta": {
        "node": "18.19.1",
        "npm": "10.2.4"
    },
    "displayName": "urban-transportation-json-new",
    "engines": {
        "vscode": "^1.67.0",
        "node": ">=18.0.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "urban-transportation",
                "aliases": [
                    "UrbanTransportation",
                    "urban-transportation"
                ],
                "extensions": [
                    ".ut"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "urban-transportation",
                "scopeName": "source.urban-transportation",
                "path": "syntaxes/urban-transportation.tmLanguage.json"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "secondaryPanel",
                    "title": "DT LLM Chat",
                    "icon": "media/images/icon.png"
                }
            ]
        },
        "views": {
            "secondaryPanel": [
                {
                    "id": "geminiUiView",
                    "name": "DT LLM Chat",
                    "type": "webview"
                }
            ]
        }
    },
    "activationEvents": [],
    "main": "./out/extension/main.cjs",
    "bin": {
        "urban-transportation-cli": "./bin/cli.js"
    }
}
